{
  "name": "bugmagnet",
  "version": "3.0.0",
  "scripts": {
    "lint": "eslint src test",
    "pretest": "npm run lint && rm -rf testem/compiled",
    "test": "testem ci -R dot",
    "pretest-browser": "npm run pretest",
    "test-browser": "testem",
    "pretest-chrome": "npm run pretest",
    "test-chrome": "testem -l Chrome",
    "sourcemap": "sourcemap-lookup",
    "prepack-extension": "rm -rf pack && cp -R template pack",
    "pack-extension": "npm run prepack-extension && webpack --config webpack.config.js --mode=production",
    "pack-extension-dev": "npm run prepack-extension && webpack --config webpack.config.js --mode=development"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "eslint": "^9.20.0",
    "eslint-config-crockford": "^2.0.0",
    "eslint-config-defaults": "^9.0.0",
    "fs-readdir-recursive": "^1.1.0",
    "globals": "^15.14.0",
    "jasmine": "^2.8.0",
    "sourcemap-lookup": "0.0.3",
    "testem": "^3.15.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-middleware": "^7.4.2"
  }
}
